				{
					"name": "Get EDR DataAddress (MQTT Broker Metadata)",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Extract MQTT broker connection metadata from EDR",
									"if(pm.response.code < 300 && pm.response.code >= 200){",
									"    const edr = pm.response.json();",
									"    pm.environment.set(\"MQTT_BROKER_URL\", edr.endpoint || edr.brokerUrl);",
									"    pm.environment.set(\"MQTT_TOPIC\", edr.topic || pm.environment.get(\"ASSET_ID\"));",
									"    if(edr.authToken){",
									"        pm.environment.set(\"MQTT_AUTH_TOKEN\", edr.authToken);",
									"    }",
									"    if(edr.username){",
									"        pm.environment.set(\"MQTT_USERNAME\", edr.username);",
									"    }",
									"    if(edr.password){",
									"        pm.environment.set(\"MQTT_PASSWORD\", edr.password);",
									"    }",
									"    console.log(\"EDR Response:\", JSON.stringify(edr, null, 2));",
									"    console.log(\"MQTT Broker URL: \" + pm.environment.get(\"MQTT_BROKER_URL\"));",
									"    console.log(\"MQTT Topic: \" + pm.environment.get(\"MQTT_TOPIC\"));",
									"}",
									"",
									"pm.test(\"Status code is >=200 and <300\", function () {",
									"    pm.expect(pm.response.code < 300 && pm.response.code >= 200).to.be.true",
